<data>
    <xpath expr="//field[@name='cost']" position="before">
        <field name="category" expand="1"/>
    </xpath>
    <xpath expr="//field[@name='cost']" position="replace">
        <field name="cost"/>
    </xpath>
    <xpath expr="//field[@name='revenue']" position="replace">
        <field name="revenue"/>
    </xpath>
</data>
